{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">date</th>
        <th scope="col">time</th>
        <th scope="col">name</th>
        <th scope="col">location</th>
        <th scope="col">numOfPeople</th>
        <th scope="col">department</th>
        <th scope="col">note</th>
    </tr>
    </thead>
    <tbody>
    {{#scheduleList}}
    <tr>
        <td >{{date}}</td>
        <td>{{time}}</td>
        <td>{{name}}</td>
        <td>{{location}}</td>
        <td>{{numOfPeople}}</td>
        <td>{{department}}</td>
        <td>{{note}}</td>
    </tr>
    {{/scheduleList}}
    </tbody>
</table>

{{>layouts/footer}}
